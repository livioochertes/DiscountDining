{
  "expo": {
    "name": "EatOff",
    "slug": "rest-express",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff6b35"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.livioochertes.restexpress",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to find nearby restaurants and provide delivery services.",
        "NSCameraUsageDescription": "This app uses camera to scan QR codes for voucher redemption.",
        "NSMicrophoneUsageDescription": "This app may use microphone for voice search features.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ff6b35"
      },
      "package": "com.livioochertes.restexpress",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [],
    "scheme": "eatoff",
    "extra": {
      "eas": {
        "projectId": "9c95bc63-78aa-424d-ac44-3d6d573ce426"
      }
    },
    "platforms": [
      "android",
      "ios"
    ],
    "owner": "livioochertes"
  }
}
